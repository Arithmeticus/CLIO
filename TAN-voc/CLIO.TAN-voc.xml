        <verb>
            <IRI>tag:textalign.net,2015:verb:resumes-at</IRI>
            <name>scriptum division starts at</name>
            <desc>The subject, a scriptum, normally with a division specified, begins just before
                the corresponding text referred to by &amp;at-ref&gt;. The claim takes no
                object.</desc>
            <constraints>
                <subject status="required" item-type="scriptum"/>
                <object status="disallowed"/>
                <at-ref status="required"/>
            </constraints>
        </verb>
